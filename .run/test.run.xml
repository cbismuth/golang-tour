<component name="ProjectRunConfigurationManager">
    <configuration default="false" name="test" type="BlazeCommandRunConfigurationType" factoryName="Bazel Command">
        <blaze-settings handler-id="BlazeGoRunConfigurationHandlerProvider" kind="go_test"
                        context-element="$PROJECT_DIR$/../src/tour_test.go" blaze-command="test">
            <blaze-target>//src:src_test</blaze-target>
            <blaze-user-flag>--test_env=GO_TEST_WRAP_TESTV=1</blaze-user-flag>
            <blaze-user-flag>--test_output=all</blaze-user-flag>
            <env_state>
                <envs/>
            </env_state>
        </blaze-settings>
        <method v="2">
            <option name="Blaze.BeforeRunTask" enabled="true"/>
        </method>
    </configuration>
</component>
